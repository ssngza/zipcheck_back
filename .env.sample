# PDF 파싱 API 환경 변수 설정

# 서버 설정
PORT=5000
DEBUG=True

# OpenAI API 설정
OPENAI_API_KEY=your_openai_api_key_here

# 파일 업로드 설정
MAX_CONTENT_LENGTH=16777216  # 16MB (16 * 1024 * 1024)

# 임시 디렉토리 설정
# TEMP_FOLDER=custom_temp_folder_path  # 기본값: [project_root]/temp
# IMAGES_FOLDER=custom_images_folder_path  # 기본값: [project_root]/temp/images 